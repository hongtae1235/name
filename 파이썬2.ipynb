        "print(\"사전 프로그램 시작... 종료는 q를 입력\")\n",
        "dict = {}\n",
        "\n",
        "while True:\n",
        "  st = input('$ ')\n",
        "  command = st[0]\n",
        "  if command == '<':\n",
        "    st = st[1:]\n",
        "    inputStr = st.split(':')\n",
        "    if len(inputStr) < 2:\n",
        "      print('입력 오류')\n",
        "    else:\n",
        "      dict[inputStr[0].strip()] = inputStr[1].strip()\n",
        "  elif command == '>':\n",
        "    st = st[1:]\n",
        "    inputStr = st.strip()\n",
        "    if inputStr in dict:\n",
        "      print(dict[inputStr])\n",
        "    else :\n",
        "      print('{}는 사전에 없음'.format(inputStr))\n",
        "  elif command == 'q':\n",
        "    break\n",
        "  else :\n",
        "    print('입력 오류')\n",
        "\n",
        "print(\"사전 프로그램 종료\")"
      ],
