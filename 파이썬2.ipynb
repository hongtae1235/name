        "import math, random\n",
        "n = int(input('정수 입력: '))\n",
        "lst = []\n",
        "for i in range(n) :\n",
        "  lst.append(random.randint(1,100))\n",
        "\n",
        "print(lst)\n",
        "mean = sum(lst) / len(lst)\n",
        "vsum = 0\n",
        "for val in lst :\n",
        "  vsum += (val - mean)**2\n",
        "variance = vsum / len(lst)\n",
        "\n",
        "print('평균 : ', mean)\n",
        "print('분산 : ', variance)\n",
        "print('표준편차 : ', math.sqrt(variance))"
      ],
